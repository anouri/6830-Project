<%= div_for query do %>
	<%= query.percentage %>%
	<% if query.kind == "SELECT" %>
		<%= query.create_format_select %>
	<% elsif query.kind == "UPDATE" %>
		<%= query.create_format_update %>
	<% elsif query.kind == "INSERT" %>
		<%= query.create_format_insert %>
	<% elsif query.kind == "DELETE" %>
		<%= query.create_format_delete %>
	<% end %>
<% end %>